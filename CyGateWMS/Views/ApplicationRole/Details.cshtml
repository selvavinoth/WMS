@model CyGateWMS.Models.ApplicationRole

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_DashBoard_Layout.cshtml";
}

    <hr />
    <div class="header">
        <ol class="breadcrumb">
            <li><a asp-action="Index" asp-controller="ApplicationRoles">Roles</a></li>
            <li><a asp-action="Index" asp-controller="ApplicationRoles" class="active">@Model.Name</a></li>
        </ol>

    </div>

    <div id="page-inner">
        <div class="row">
            <div class="col-md-12">
                <!-- Advanced Tables -->
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Advanced Tables
                    </div>
                    <div class="panel-body">
                        <div class="table-responsive">
                            <div id="dataTables-example_wrapper" class="dataTables_wrapper form-inline" role="grid">

                                <table class="table table-striped table-bordered table-hover dataTable no-footer" id="dataTables-example" aria-describedby="dataTables-example_info">


                                    <tbody>
                                        <tr class="gradeA odd">
                                            <td class="sorting_1"> @Html.DisplayNameFor(model => model.Description)</td>
                                            <td class="center ">@Html.DisplayFor(model => model.Description)</td>
                                        </tr>
                                        <tr class="gradeA even">
                                            <td class="sorting_1"> @Html.DisplayNameFor(model => model.Name)</td>
                                            <td class="center ">@Html.DisplayFor(model => model.Name)</td>
                                        </tr>
                                        <tr class="gradeA even">
                                            <td class="sorting_1"> @Html.DisplayNameFor(model => model.CreatedDate)</td>
                                            <td class="center ">@Html.DisplayFor(model => model.CreatedDate)</td>
                                        </tr>
                                        <tr class="gradeA even">
                                            <td class="sorting_1"> @Html.DisplayNameFor(model => model.IsActive)</td>
                                            <td class="center ">@Html.DisplayFor(model => model.IsActive)</td>
                                        </tr>
                                        <tr class="gradeA even">
                                            <td class="sorting_1"> @Html.DisplayNameFor(model => model.Id)</td>
                                            <td class="center ">@Html.DisplayFor(model => model.Id)</td>
                                        </tr>
                                    </tbody>
                                </table><div class="row">
                                </div>
                            </div>
                            <div>
                                <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
                                <a asp-action="Index">Back to List</a>
                            </div>
                        </div>
                </div>
                <!--End Advanced Tables -->
            </div>
        </div>
</div>
    </div>